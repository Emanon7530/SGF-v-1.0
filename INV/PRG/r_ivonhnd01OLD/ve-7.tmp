    Private Sub LibXConnector1_BeforeExecuteQuery(ByVal sender As Object, ByVal e As LibX.XBeforeExecuteQueryEventArgs) Handles LibXConnector1.BeforeExecuteQuery
        Dim xSelect As LibX.Data.XSelecStmt
        Dim oTable As DataTable
        Dim FechaBalanceInicial As Object
        Dim SelectStmt As String
        Dim ID As Integer
        Try

            e.DoFill = False

            If Me.fecHasta.Value.ToString.Trim.Length <= 0 Then
                Throw New ApplicationException("Debe especificar la fecha de cierre")
            End If

            If Not IsDate(Me.fecHasta.Value) Then
                Throw New ApplicationException("Debe especificar una fecha válida")
            End If

            If CType(fecHasta.Value, Date) > LibX.Data.Manager.Connection.GetDate Then
                Throw New ApplicationException("La Fecha no puede ser mayor a la fecha del día")
            End If


            SelectStmt = "Select max(cierr_date) from ivcierrm " & _
                        " where cierr_date <= '" & CDate(Me.fecHasta.Value).ToString(LibX.Data.Manager.GetSaveDateFormat) & "'"

            '" where cierr_date <= '" & Me.fecHasta.Value & "'"

            FechaBalanceInicial = LibX.Data.Manager.GetScalar(SelectStmt)

            If IsDBNull(FechaBalanceInicial) Then
                Throw New ApplicationException("No existen cierres para esta fecha")
            End If

            'SelectStmt = "Select ivcierrm.item_code,ivcierrm.whse_code,ivitemv1.item_name,ivitemv1.unit_code,ivitemv1.Ubicacion, " & _
            '             " ivcierrm.onhand,purch_cost,price " & _
            '             " from ivcierrm, ivitemv1 " & _
            '             " where cierr_date = '" & CType(FechaBalanceInicial, Date).ToString(LibX.Data.Manager.GetSaveDateFormat) & "'" & _
            '             "   and ivcierrm.item_code = ivitemv1.item_code " & _
            '             "   and ivcierrm.whse_code = ivitemv1.whse_code "
            SelectStmt = "SELECT ivitemd.whse_code,ivitemm.item_code,ivitemm.item_name,ivitemd.purch_cost,ivitemm.unit_code," & _
                         " ivitemd.price,ivcierrm.onhand,rtrim(ivitemd.estante) + '-' +  cast(ivitemd.loc_row as varchar) + '-' +  cast(ivitemd.loc_col AS VARCHAR) Ubicacion" & _
                         " FROM ivitemm inner JOIN ivitemd " & _
                         " ON ivitemm.item_code = ivitemd.item_code" & _
                         " left outer join ivcierrm on  ivcierrm.item_code = ivitemd.item_code" & _
                         " and ivcierrm.whse_code = ivitemd.whse_code" & _
                         " and ivcierrm.cierr_date = '" & CType(FechaBalanceInicial, Date).ToString(LibX.Data.Manager.GetSaveDateFormat) & "'"

            xSelect = New LibX.Data.XSelecStmt("ivcierrm", SelectStmt)

            oTable = xSelect.GetTable(False)
            xSelect.Dispose()

            ProgressBar1.Maximum = oTable.Rows.Count + 1
            ProgressBar1.Minimum = 1
            ProgressBar1.Value = 1

            LibX.Data.Manager.Connection.BeginTransaction()

            ID = Val(DateTime.Now.Millisecond.ToString & DateTime.Now.Second.ToString & _
                 DateTime.Now.Minute.ToString & DateTime.Now.Hour.ToString)

            For Each oRow As DataRow In oTable.Rows
                Dim oNewRow As DataRow = ivitemr.NewRow

                oNewRow!itemid = ID
                oNewRow!item_code = oRow!item_code
                oNewRow!item_name = oRow!item_name
                oNewRow!whse_code = oRow!whse_code
                oNewRow!costus = oRow!purch_cost
                oNewRow!unidads = oRow!unit_code
                oNewRow!priceus = oRow!price
                oNewRow!ubicac = oRow!Ubicacion
                'oNewRow!costtotalrd = oRow!onhand * oNewRow!costrd
                oNewRow!onhand = AplicarMovimientos(oRow!item_code.ToString, CDate(FechaBalanceInicial), CDate(Me.fecHasta.Value), Val(oRow!onhand.ToString))

                If oNewRow!onhand <> 0 Then

                    ivitemr.Rows.Add(oNewRow)


                End If
                ProgressBar1.Value += 1
            Next

            LibX.Data.Manager.Save(ivitemr)
            LibX.Data.Manager.Connection.CommitTransaction()

            If ivitemr.Rows.Count > 0 Then
                imprimir()
            End If

        Catch ex As Exception
            Log.Show(ex)
        End Try
    End Sub

    Private Function AplicarMovimientos(ByVal item_code As String, ByVal fecha As Date, ByVal fechaf As Date, ByVal existencia As Integer) As Integer
        Dim SelectStmt As String
        Dim oTable As DataTable

        Try

            SelectStmt = "select ivtranrd.item_code,qty,ivtranrm.type_code,origen,ivunitd.factor" & _
                        " from ivtranrd inner join ivtranrm" & _
                        " on ivtranrd.trane_number= ivtranrm.trane_number" & _
                        " inner join ivunitd" & _
                        " on ivtranrd.unit_code = ivunitd.unit_code" & _
                        " and ivtranrd.item_code = ivunitd.item_code" & _
                        " and ivtranrm.doc_date > '" & fecha.ToString(LibX.Data.Manager.GetSaveDateFormat) & _
                        "' and ivtranrm.doc_date <= '" & fechaf.ToString(LibX.Data.Manager.GetSaveDateFormat) & _
                        "' inner join ivtypem " & _
                        " on ivtranrm.type_code = ivtypem.type_code" & _
                        " where ivtranrd.item_code = '" & item_code.Trim & "'"


            oTable = LibX.Data.Manager.GetDataTable(SelectStmt)

            For Each orow As DataRow In oTable.Rows
                existencia += (Val(orow!qty.ToString) * Val(orow!origen.ToString)) * Val(orow!factor.ToString)
            Next

            Return existencia

        Catch ex As Exception
            LibX.Log.Show(ex)
            Return -1
        End Try
    End Function

