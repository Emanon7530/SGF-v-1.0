    Private Sub imprimir()
        Dim oRep As LibX.ReportLib
        Try
            oRep = New LibX.ReportLib("INV", "r_ivonhnd01.rpt")
            With oRep
                .ParameterFields(1) = "FechaCierre;" & Me.fecHasta.Value & ";TRUE"
                .Action = 1
            End With
        Catch ex As Exception
            LibX.Log.Show(ex)
        End Try
    End Sub

